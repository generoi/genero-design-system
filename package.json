{
  "name": "genero-design-system",
  "version": "0.0.1",
  "description": "Genero Design System",
  "license": "MIT",
  "repository": "generoi/genero-design-system",
  "homepage": "https://github.com/generoi/genero-design-system",
  "bugs": "https://github.com/generoi/genero-design-system/issues",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/button/button.js",
  "files": [
    "dist/",
    "loader/",
    "src/**/*.scss",
    "scss/"
  ],
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --write .",
    "build": "npm-run-all build:* lint:prettier",
    "build:stencil": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "engines": {
    "node": ">=11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@stencil/core": "^1.13.0",
    "@stencil/sass": "^1.3.1",
    "@storybook/web-components": "^5.3.18",
    "babel-loader": "^8.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lit-html": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5"
  }
}
