{
  "name": "genero-design-system",
  "version": "0.11.0",
  "description": "Genero Design System",
  "license": "MIT",
  "repository": "generoi/genero-design-system",
  "homepage": "https://github.com/generoi/genero-design-system",
  "bugs": "https://github.com/generoi/genero-design-system/issues",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/gds/gds.js",
  "files": [
    "dist/",
    "loader/",
    "react/",
    "src/**/*.scss",
    "src/**/*.css"
  ],
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --write .",
    "build": "npm-run-all lint:prettier build:*",
    "build:stencil": "stencil build --docs",
    "build:storybook": "build-storybook -s public -o docs --loglevel verbose",
    "build:react-ts": "tsc -p react-ts/",
    "start": "stencil build --dev --docs --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -s ./public -p 6006",
    "prepare": "npm run build",
    "release": "standard-version"
  },
  "engines": {
    "node": "12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@stencil/core": "^1.13.0",
    "@stencil/react-output-target": "0.0.6",
    "@stencil/sass": "^1.3.1",
    "@storybook/addon-docs": "^6.0.0-beta.36",
    "@storybook/web-components": "^6.0.0-beta.36",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lit-html": "^1.2.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^8.0.2",
    "standard-version": "^8.0.0",
    "style-loader": "^1.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "storybook-deployer": {
    "commitMessage": "docs: deploy storybook to gitHub pages"
  }
}
