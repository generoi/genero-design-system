{
  "name": "genero-design-system",
  "version": "4.0.0-beta.3",
  "description": "Genero Design System",
  "license": "MIT",
  "repository": "generoi/genero-design-system",
  "homepage": "https://github.com/generoi/genero-design-system",
  "bugs": "https://github.com/generoi/genero-design-system/issues",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/gds/gds.js",
  "files": [
    "dist/",
    "loader/",
    "react/",
    "src/**/*.scss",
    "src/**/*.tsx",
    "src/**/*.svg",
    "src/**/*.css",
    "stencil.config.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --debug-check .",
    "build": "npm-run-all lint:prettier build:stencil",
    "build:stencil": "stencil build --docs",
    "build:storybook": "build-storybook -s public -o docs --loglevel verbose",
    "start": "ncu && stencil build --docs --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -s ./public -p 6006",
    "prepare": "npm run build",
    "release": "standard-version",
    "check-updates": "ncu"
  },
  "engines": {
    "node": "12 || 14"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@commitlint/cli": "^12.1.0",
    "@commitlint/config-conventional": "^12.1.0",
    "@panosvoudouris/addon-versions": "^1.0.1",
    "@stencil/core": "^2.5.1",
    "@stencil/postcss": "^2.0.0",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/web-components": "^6.1.21",
    "@types/autoprefixer": "^10.2.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "lit-html": "^1.3.0",
    "npm-check-updates": "^11.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "standard-version": "^9.1.1",
    "stencil-inline-svg": "^1.0.1",
    "style-loader": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "storybook-deployer": {
    "commitMessage": "docs: deploy storybook to gitHub pages"
  }
}
